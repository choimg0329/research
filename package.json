{
  "name": "research_backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "env:dev": "cross-env NODE_ENV=dev dotenv_config_path=config/.dev.env",
    "env:prod": "cross-env NODE_ENV=prod dotenv_config_path=config/.prod.env",
    "env:local": "cross-env NODE_ENV=local dotenv_config_path=config/.local.env",
    "env:ss": "cross-env NODE_ENV=ss dotenv_config_path=config/.ss.env",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=dev dotenv_config_path=config/.dev.env node -r dotenv/config dist/src/main",
    "start:prod": "cross-env NODE_ENV=prod dotenv_config_path=config/.prod.env node -r dotenv/config dist/src/main",
    "start:local": "cross-env NODE_ENV=local dotenv_config_path=config/.local.env node -r dotenv/config dist/src/main",
    "start:ss": "cross-env NODE_ENV=ss dotenv_config_path=config/.ss.env node -r dotenv/config dist/src/main",
    "start:debug": "nest start --debug --watch",
    "dev:start": "cross-env NODE_ENV=dev dotenv_config_path=config/.dev.env nest start --watch",
    "dev:local": "cross-env NODE_ENV=local dotenv_config_path=config/.local.env nest start --watch",
    "dev:ss": "cross-env NODE_ENV=ss dotenv_config_path=config/.ss.env nest start --watch",
    "start:build:dev": "npm run build && npm run start:dev",
    "start:build:local": "npm run build && npm run start:local",
    "start:build:ss": "npm run build && npm run start:ss",
    "start:prod:env": "cross-env NODE_ENV=prod node dist/main",
    "start:test:env": "cross-env NODE_ENV=test nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate:dev": "npx prisma migrate dev --env-file config/.dev.env",
    "prisma:migrate:prod": "npx prisma migrate deploy --env-file config/.prod.env",
    "prisma:migrate:local": "npx prisma migrate dev --env-file config/.local.env",
    "prisma:migrate:ss": "npx prisma migrate dev --env-file config/.ss.env",
    "prisma:generate:dev": "npx prisma generate --env-file config/.dev.env",
    "prisma:generate:prod": "npx prisma generate --env-file config/.prod.env",
    "prisma:generate:local": "npx prisma generate --env-file config/.local.env",
    "prisma:generate:ss": "npx prisma generate --env-file config/.ss.env",
    "erd:generate": "npx cross-env dotenv_config_path=config/.dev.env npx prisma generate",
    "erd:studio": "npx cross-env dotenv_config_path=config/.dev.env npx prisma studio",
    "erd:view": "open prisma/ERD.svg",
    "erd:readme": "cat prisma/ERD.md",
    "studio:dev": "cp config/.dev.env .env && npx prisma studio",
    "studio:prod": "cp config/.prod.env .env && npx prisma studio",
    "studio:local": "cp config/.local.env .env && npx prisma studio",
    "studio:ss": "cp config/.ss.env .env && npx prisma studio",
    "pm2:start:dev": "pm2 start ecosystem.config.js --only research-backend-dev",
    "pm2:start:prod": "pm2 start ecosystem.config.js --only research-backend-prod",
    "pm2:start:local": "pm2 start ecosystem.config.js --only research-backend-local",
    "pm2:start:ss": "pm2 start ecosystem.config.js --only research-backend-ss",
    "pm2:stop:dev": "pm2 stop research-backend-dev",
    "pm2:stop:prod": "pm2 stop research-backend-prod",
    "pm2:stop:local": "pm2 stop research-backend-local",
    "pm2:stop:ss": "pm2 stop research-backend-ss",
    "pm2:restart:dev": "pm2 restart research-backend-dev",
    "pm2:restart:prod": "pm2 restart research-backend-prod",
    "pm2:restart:local": "pm2 restart research-backend-local",
    "pm2:restart:ss": "pm2 restart research-backend-ss",
    "pm2:logs:dev": "pm2 logs research-backend-dev",
    "pm2:logs:prod": "pm2 logs research-backend-prod",
    "pm2:logs:local": "pm2 logs research-backend-local",
    "pm2:logs:ss": "pm2 logs research-backend-ss",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "^6.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.12.0",
    "prisma-erd-generator": "^2.0.4",
    "puppeteer": "^24.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
